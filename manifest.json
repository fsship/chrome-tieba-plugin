{
    "name": "TBPlugin",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "贴吧插件",
    "permissions": [
        "http://tieba.baidu.com/",
        "storage"
    ],
    "content_scripts":[
		{
			"matches":["http://tieba.baidu.com/*"],
			"js":["js/remover.js"],
			"run_at":"document_end"
		}
	],
    "browser_action": {
        "default_icon": "favicon.ico" ,
        "default_title": "TBPlugin",
        "default_popup": "popup.html"
    }
}
